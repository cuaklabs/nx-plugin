{
  "name": "@cuaklabs/nx-plugin",
  "version": "0.1.0",
  "description": "Cuaklabs nx plugin",
  "scripts": {
    "build": "tsc",
    "build:clean": "rimraf lib",
    "commit": "commit",
    "prebuild": "pnpm run build:clean",
    "prepare": "husky install",
    "test:js": "jest --config=config/jest/jest.js.config.mjs --selectProjects Unit Integration",
    "test:uncommitted": "pnpm run test --changedSince=HEAD",
    "test": "jest --config=config/jest/jest.config.mjs --selectProjects Unit Integration"
  },
  "keywords": [
    "nx",
    "plugin"
  ],
  "author": "Cuak McCuackl <cuak.mccuakl@cuaklabs.com>",
  "license": "See license in \"LICENSE\" file",
  "devDependencies": {
    "@commitlint/cli": "17.1.2",
    "@commitlint/config-conventional": "17.1.0",
    "@commitlint/prompt-cli": "17.1.2",
    "@jest/globals": "^29.0.1",
    "@types/node": "16",
    "@typescript-eslint/eslint-plugin": "5.36.1",
    "@typescript-eslint/parser": "5.36.1",
    "eslint": "8.23.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jest": "27.0.1",
    "eslint-plugin-prettier": "4.2.1",
    "husky": "8.0.1",
    "jest": "^29.0.1",
    "jest-mock": "^29.0.1",
    "prettier": "2.7.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^28.0.8",
    "ts-node": "^10.9.1",
    "typescript": "4.8.2"
  },
  "main": "lib/index.js",
  "dependencies": {
    "@nrwl/devkit": "^14.5.10",
    "nx": "^14.5.10"
  }
