{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "Run Commands",
  "description": "Run any custom commands in interactive mode (stdin, stout and stderr are piped!).",
  "type": "object",
  "additionalProperties": false,
  "properties": {
    "commands": {
      "type": "array",
      "items": {
        "type": "string"
      }
    },
    "cwd": {
      "type": "string"
    },
    "parallel": {
      "type": "boolean"
    }
  }
}
